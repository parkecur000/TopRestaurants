@{
    ViewBag.Title = "Top 5 Restaurants";
}

@model IEnumerable<string>

<div class="list-group">
    <ul>
        @foreach (string i in Model) //Model in this instance is referring to whatever object you passed in in the controller when you called the view
        {
            <li class="list-group-item">@Html.Raw(i)</li>
        }
    </ul>
</div>